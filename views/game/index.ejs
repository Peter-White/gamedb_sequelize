<%- include('../header') -%>
<div class="container">
  <div class="row">
    <div class="col-md-12">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Title</th>
            <th scope="col">Released</th>
          </tr>
        </thead>
        <tbody>

          <% games.forEach(function(game, index) { %>
            <tr>
              <th scope="row"><%= index + 1 %></th>
              <td><a href="/games/<%= game.id %>"><%= game.title %></a></td>
              <td><%= game.released %></td>
            </tr>
          <% }) %>

        </tbody>
      </table>
    </div>
  </div>
</div>
<%- include('../jquery') -%>
<%- include('../datatable') -%>
<%- include('../footer') -%>
